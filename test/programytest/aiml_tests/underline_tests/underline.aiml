	<category>
		<pattern>_ HEY</pattern>
		<template>
		    STAR IS <star index="1" />
		</template>
	</category>

	<category>
		<pattern>HELLO _</pattern>
		<template>
		    HI <star index="1" />
		</template>
	</category>

	<category>
		<pattern>GOODBYE _ SEEYA</pattern>
