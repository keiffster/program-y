	</category>

	<category>
		<pattern>_ HI _</pattern>
		<template>
		    YOU SAID <star index="1" /> AND <star index="2" />
		</template>
	</category>

    <category>
        <pattern>_ _STOP_</pattern>
        <template><star index="1"></star></template>
    </category>

</aiml>