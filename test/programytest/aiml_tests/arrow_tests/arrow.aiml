<aiml>

	<category>
		<pattern>^ HEY</pattern>
		<template>
		    ARROW IS <star index="1" />
		</template>
	</category>

	<category>
		<pattern>^ CHECKING MULTI WORD MATCH</pattern>
		<template>
		    ARROW IS NOW <star index="1" />
		</template>
	</category>

	<category>
		<pattern>HELLO ^</pattern>
		<template>
		    ARROW IS <star index="1" />
		</template>
	</category>

	<category>
		<pattern>WELL ^ THERE</pattern>
		<template>
		    ARROW IS <star index="1" />
		</template>
	</category>

	<category>
		<pattern>TEST1 ^ TEST2 ^</pattern>
		<template>answer 2</template>
	</category>

	<category>
		<pattern>TEST1 ^</pattern>
		<template>answer 1</template>
	</category>

	<category>
		<pattern>TEST3 ^</pattern>
		<template>answer 3</template>
	</category>

</aiml>