<aiml>

	<category>
		<pattern>HELLO</pattern>
		<template>
		    HI THERE
		</template>
	</category>

    <category>
        <pattern>HELLO AGAIN</pattern>
        <template>
            HELLO WITHOUT THAT
        </template>
    </category>

    <category>
        <pattern>HELLO AGAIN</pattern>
        <that>HI THERE</that>
        <template>
            HELLO WITH THAT
        </template>
    </category>

    <category>
        <pattern>WELCOME</pattern>
        <template>aaa bbb ccc ddd</template>
    </category>

    <category>
        <pattern>*</pattern>
        <that>^ ddd</that>
        <template>matched</template>
    </category>

    <category>
        <pattern>WELCOME2</pattern>
        <template>xxx yyy zzz</template>
    </category>

    <category>
        <pattern>*</pattern>
        <that># zzz</that>
        <template>matched2</template>
    </category>

</aiml>