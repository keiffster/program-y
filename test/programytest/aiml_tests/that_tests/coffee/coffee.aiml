<?xml version="1.0" encoding="ISO-8859-1"?>

<aiml version="2.0">
    <category>
        <pattern>I LIKE COFFEE</pattern>
        <template>DO YOU TAKE CREAM OR SUGAR IN YOUR COFFEE?</template>
    </category>

    <category>
        <pattern>YES</pattern>
