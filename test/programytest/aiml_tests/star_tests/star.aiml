<aiml>

	<category>
		<pattern>* HEY</pattern>
		<template>
		    STAR IS <star index="1" />
		</template>
	</category>

	<category>
		<pattern>HELLO *</pattern>
		<template>
		    HI <star index="1" />
		</template>
	</category>

	<category>
		<pattern>GOODBYE * SEEYA</pattern>
		<template>
		    LATER <star/>
		</template>
	</category>

	<category>
		<pattern>* HI *</pattern>
		<template>
		    YOU SAID <star index="1" /> AND <star index="2" />
		</template>
	</category>

	<category>
		<pattern>* HI2 * *</pattern>
		<template>
		    YOU SAID <star index="1" /> AND <star index="2" /> THEN <star index="3" />
		</template>
	</category>

	<category>
		<pattern>MULTIPLE STARS MATCH * *</pattern>
		<template>
		    YOU MATCHED FIRST AS <star index="1" /> AND SECOND AS <star index="2" />
		</template>
	</category>

	<category>
		<pattern>MULTI STARS * * * *</pattern>
		<template>
		    <star index="4" /> <star index="3" /> <star index="2" /> <star index="1" />
		</template>
	</category>

	<category>
		<pattern>STARS FIRST * * SECOND *</pattern>
		<template>
		    3=<star index="3" /> 2=<star index="2" /> 1=<star index="1" />
		</template>
	</category>

	<category>
		<pattern>STAR WITH SET <set>number</set></pattern>
		<template>
		    SET IS <star/>
		</template>
	</category>

	<category>
		<pattern>STAR WITH SETS <set>number</set> <set>number</set></pattern>
		<template>
		    SETS ARE <star/> AND <star index="2"/>
		</template>
	</category>

	<category>
		<pattern>MIXED STARS AND SETS * <set>number</set> * <set>number</set> *</pattern>
		<template>
		    STARS ARE <star/> AND <star index="2"/> AND <star index="3"/> AND <star index="4"/> AND <star index="5"/>
		</template>
	</category>

	<category>
		<pattern>STAR CASE *</pattern>
		<template>
		    <star/>
		</template>
	</category>

</aiml>