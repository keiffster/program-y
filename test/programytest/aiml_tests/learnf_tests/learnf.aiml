<aiml>

    <category>
        <pattern>MY NAME IS *</pattern>
        <template>
            <think>
                <set name="myname">
                    <star/>
                </set>
            </think>
            <learnf>
                <category>
                    <pattern>WHAT IS MY NAME</pattern>
                    <template>YOUR NAME IS
                        <eval>
                            <get name="myname"/>
                        </eval>
                    </template>
                </category>
            </learnf>
            OK, I will remember your name is
            <get name="myname"/>
        </template>
    </category>

    <category>
        <pattern>* PLAYED *</pattern>
        <template>Ok. I will remember this
            <learnf>
                <category>
                    <pattern>WHAT DID
                        <eval>
                            <star/>
                            PLAY
                        </eval>
                    </pattern>
                    <template>
                        <eval>
                            <star/>
                        </eval>
                        PLAYED
                        <eval>
                            <star index="2"/>
                        </eval>
                    </template>
                </category>
            </learnf>
        </template>
    </category>

</aiml>