    <category>
        <pattern>* IS <set>erdown</set> THAN *
        </pattern>
        <template>
            <think>
                <set var="erup">
                    <star index="2"/>
                </set>
                <set var="opposite">
                    <map>
                        <name>opposite</name>
                        <get var="erup"/>
                    </map>
                </set>
                <set var="thing1">
                    <star/>
                </set>
                <set var="thing2">
                    <star index="3"/>
                </set>
            </think>
            <srai>
                <get var="thing2"/>
                IS
                <get var="opposite"/>
                THAN
                <get var="thing1"/>
            </srai>
        </template>
