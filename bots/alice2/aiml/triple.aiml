    <category>
		<pattern>TRIPLE TEST * *</pattern>
		<template>
			<select>
				<vars>?x</vars>
				<q><subj><star index="1"/></subj><pred><star index="2"/></pred><obj>?x</obj></q>
			</select>
		</template>
	</category>

    <category>
        <pattern>GENSYM</pattern>
        <template>
            <condition name="symcount">
                <li value="unknown">symbol
                    <set name="symcount">0</set>
                </li>
                <li>symbol
                    <set name="symcount">
                        <map name="successor">
                            <get name="symcount"/>
                        </map>
                    </set>
                </li>
            </condition>
        </template>
    </category>

    <category>
        <pattern>I HAVE A <set>color</set> *
        </pattern>
        <template>
            <think>
                <set var="color">
                    <star/>
                </set>
                <set var="noun">
                    <star index="2"/>
                </set>
                <set var="symbol">
                    <srai>GENSYM</srai>
                </set>
                <set var="obj">
                    <star index="2"/>
                </set>
                <set var="description">a
                    <star/>
                    <star index="2"/>
                </set>
                <addtriple>
                    <subj>
                        <id/>
                    </subj>
                    <pred>has</pred>
                    <obj>
                        <get var="symbol"/>
                    </obj>
                </addtriple>
                <addtriple>
                    <subj>
                        <get var="symbol"/>
                    </subj>
                    <pred>hasColor</pred>
                    <obj>
                        <get var="color"/>
                    </obj>
                </addtriple>
                <addtriple>
                    <subj>
                        <get var="symbol"/>
                    </subj>
                    <pred>hasDescription</pred>
                    <obj>
                        <get var="description"/>
                    </obj>
                </addtriple>
                <addtriple>
                    <subj>
                        <get var="symbol"/>
                    </subj>
                    <pred>isA</pred>
                    <obj>
                        <get var="obj"/>
                    </obj>
                </addtriple>
            </think>
            <star/>
            is a nice color of<star index="2"/>.
        </template>
    </category>

    <category>
        <pattern>WHAT DO I HAVE ^</pattern>
        <template>
            <set var="tuples">
                <select>
                    <vars>?sym</vars>
                    <q>
                        <subj>
                            <id/>
                        </subj>
                        <pred>has</pred>
                        <obj>?sym</obj>
                    </q>
                </select>
            </set>
            <condition var="tuples">
                <li value="NIL">I don't know. What do you have?</li>
                <li>
                    <condition var="tuples">
                        <li value="NIL"/>
                        <li>
                            <think>
                                <set var="head">
                                    <first>
                                        <get var="tuples"/>
                                    </first>
                                </set>
                                <set var="tuples">
                                    <rest>
                                        <get var="tuples"/>
                                    </rest>
                                </set>
                            </think>
                            <uniq>
                                <subj>
                                    <get var="?sym">
                                        <tuple>
                                            <get var="head"/>
                                        </tuple>
                                    </get>
                                </subj>
                                <pred>hasDescription</pred>
                                <obj>?description</obj>
                            </uniq>
                            <condition var="tuples">
                                <li value="NIL"/>
                                <li>,</li>
                            </condition>
                            <loop/>
                        </li>
                    </condition>
                </li>
            </condition>
        </template>
    </category>

    <category>
        <pattern>WHAT COLOR IS MY *</pattern>
        <template>
            <think>
                <set var="tuples">
                    <select>
                        <vars>?color</vars>
                        <q>
                            <subj>
                                <id/>
                            </subj>
                            <pred>has</pred>
                            <obj>?sym</obj>
                        </q>
                        <q>
                            <subj>?sym</subj>
                            <pred>isA</pred>
                            <obj>
                                <star/>
                            </obj>
                        </q>
                        <q>
                            <subj>?sym</subj>
                            <pred>hasColor</pred>
                            <obj>?color</obj>
                        </q>
                    </select>
                </set>
            </think>
            <condition var="tuples">
                <li value="NIL">You have a<star/>?
                </li>
                <li>
                    <get var="?color">
                        <tuple>
                            <first>
                                <get var="tuples"/>
                            </first>
                        </tuple>
                    </get>
                    .
                </li>
            </condition>
        </template>
    </category>

    <category>
        <pattern>WHAT <set>color</set> ^ DO I HAVE
        </pattern>
        <template>
            <think>
                <set var="tuples">
                    <select>
                        <vars>?obj</vars>
                        <q>
                            <subj>
                                <id/>
                            </subj>
                            <pred>has</pred>
                            <obj>?sym</obj>
                        </q>
                        <q>
                            <subj>?sym</subj>
                            <pred>hasColor</pred>
                            <obj>
                                <star/>
                            </obj>
                        </q>
                        <q>
                            <subj>?sym</subj>
                            <pred>isA</pred>
                            <obj>?obj</obj>
                        </q>
                    </select>
                </set>
            </think>
            <condition var="tuples">
                <li value="NIL">Nothing that I know of.</li>
                <li>Your
                    <get var="?obj">
                        <tuple>
                            <first>
                                <get var="tuples"/>
                            </first>
                        </tuple>
                    </get>
                    .
                </li>
            </condition>
        </template>
    </category>

    <category>
        <pattern>
            <set>entity</set>
        </pattern>
        <template>
            <star/>
            is an enity.
        </template>
    </category>

    <category>
        <pattern>SIZE TABLE</pattern>
        <template>
            <think>
                <set var="size">0</set>
            </think>
            <condition var="size">
                <li value="16"/>
                <li><get var="size"/>:
                    <think>
                        <set var="items">
                            <select>
                                <vars>?x</vars>
                                <q>
                                    <subj>?x</subj>
                                    <pred>hasSize</pred>
                                    <obj>
                                        <get var="size"/>
                                    </obj>
                                </q>
                            </select>
                        </set>
                        <set var="size">
                            <map>
                                <name>successor</name>
                                <get var="size"/>
                            </map>
                        </set>
                    </think>
                    <condition var="items">
                        <li value="NIL"/>
                        <li>
                            <think>
                                <set var="head">
                                    <first>
                                        <get var="items"/>
                                    </first>
                                </set>
                                <set var="items">
                                    <rest>
                                        <get var="items"/>
                                    </rest>
                                </set>
                            </think>
                            "
                            <get var="?x">
                                <tuple>
                                    <get var="head"/>
                                </tuple>
                            </get>
                            "
                            <srai>COMMAFY
                                <get var="items"/>
                            </srai>
                            <loop/>
                        </li>
                    </condition>
                    <loop/>
                </li>
            </condition>
        </template>
    </category>

    <category>
        <pattern>COMMAFY NIL</pattern>
        <template>
            <template/>
        </template>
    </category>

    <category>
        <pattern>COMMAFY *</pattern>
        <template>,</template>
    </category>

