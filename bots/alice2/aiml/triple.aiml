    <category>
        <pattern>WHICH IS LARGER ^ <set>entity</set> OR ^
            <set>entity</set>
        </pattern>
        <template>
            <think>
                <set var="larger">
                    <srai>LARGEROF
                        <star index="2"/>
                        AND
                        <star index="4"/>
                    </srai>
                </set>
            </think>
            <condition var="larger">
                <li value="unknown">I can't tell which one is larger.</li>
                <li value="same">They are about the same size.</li>
                <li>
                    <formal>
                        <get var="larger"/>
                    </formal>
                    is larger.
                </li>
            </condition>
        </template>
    </category>

